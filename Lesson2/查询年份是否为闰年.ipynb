    "year = 1\n",
    "while year != 0:\n",
    "    year = input(\"请输入年份：\")\n",
    "    try:\n",
    "        if int(year) == 0:\n",
    "            break\n",
    "        elif int(year)%4 == 0:\n",
    "            print(\"该年是闰年,退出请输入0\")\n",
    "        else:\n",
    "            print(\"该年不是闰年，退出请输入0\")\n",
    "    except:\n",
    "        print(\"你输入的不是年份，退出请输入0\")\n",
    "print(\"已退出！\")"
   ]
  }
